<goml width="512" height="512">
  <import-material typeName="lambart" src="./index.sort" />
  <geometry name="cube" type="cube" />
  <material id="sprite" type="lambart" color="#cc3" sun="0.5,0.5" />
  <renderer camera=".camera" viewport="0,0,100%,100%">
    <render-scene/>
  </renderer>
  <scene>
    <camera class="camera" near="0.01" far="40.0" aspect="1.0" fovy="45d" position="0,0,20" rotation="y(0d)">
      <camera.components>
        <MouseCameraControl rotateX="10" moveSpeed="1" />
      </camera.components>
    </camera>
    <mesh position="0,0,0" geometry="cube" material="#sprite" targetBuffer="default" />
    <mesh position="5,0,0" geometry="cube" material="#sprite" targetBuffer="default" />
    <mesh position="-5,0,0" geometry="cube" material="#sprite" targetBuffer="default" />
  </scene>
</goml>
