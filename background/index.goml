<goml width="480" height="320">
    <import-material type="color" src="index.sort" />
    <geometry name="circle" type="circle" divide="6" />
    <material id="color" type="color" />
    <scene>
        <camera class="camera" near="0.01" far="40.0" aspect="1.5" fovy="45d" position="0,0,10" />
        <mesh class="circle" position="1,0,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="-1,0,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="3,0,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="-3,0,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="0,2,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="0,-2,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="2,2,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="2,-2,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="-2,2,0" geometry="circle" material="#color" rotation="0,0,0" />
        <mesh class="circle" position="-2,-2,0" geometry="circle" material="#color" rotation="0,0,0" />
    </scene>
</goml>