@Pass{
@BlendFunc(ONE,ONE)
FS_PREC(mediump,float)
#ifdef VS
  attribute vec3 position;
  uniform mat4 _matPVM;
  void main(){
    gl_Position = _matPVM * vec4(position,1);
  }
#endif

#ifdef FS
  @{type:"color", default:"#381794"}
  uniform vec4 color;
  uniform float _time;
  void main(){
    gl_FragColor = color;
  }
#endif
}
