<goml width="512" height="512">
  <import-material type="raymarching" src="./raymarching.sort"/>
    <import-material type="red" src="./gray.sort"/>
  <geometry name="quad" type="quad"/>
  <material id="solid" type="solid" color="green"/>
  <renderer viewport="0,0,100%,100%">
    <texture-buffer name="target"/>
    <render-quad out="target" material="new(raymarching)" lightDir="0,0,1"/>
    <render-quad material="new(red)" source="backbuffer(target)"/>
  </renderer>
</goml>
